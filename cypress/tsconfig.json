{
  "compilerOptions": {
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "module": "esnext",
    "moduleResolution": "bundler",
    "target": "es2017",
    "lib": ["esnext", "dom"],
    "noEmit": true,
    "types": ["cypress", "node"]
  },
  "include": [
    "e2e/**/*",
    "support/**/*"
  ]
}